    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsdoc": "^30.0.3",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "twitter-d": "^0.4.0",
    "typescript": "^3.9.7"
  },
